FUNCTION fact :
PARAM t0
IF t0 != #1 GOTO L0
RETURN t0
GOTO L1
LABEL L0 :
t3 :=  t0 - #1
ARG t3
t2 := CALL fact
t1 :=  t0 * t2
RETURN t1
LABEL L1 :

FUNCTION main :
t4 := CALL read
IF t4 <= #1 GOTO L2
ARG t4
t5 := CALL fact
GOTO L3
LABEL L2 :
t5 := #1
LABEL L3 :
ARG t5
t6 := CALL write
RETURN #0

FUNCTION read :
READ t7
RETURN t7

FUNCTION write :
PARAM t8
WRITE t8
RETURN #0
